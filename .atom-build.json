{
  "cmd": "scons",
  "targets": {
    "build-release": {
      "cmd": "scons --build-release",
      "sh": true,
      "cwd": "{PROJECT_PATH}"
    },
    "build-debug": {
      "cmd": "scons --build-debug",
      "sh": true,
      "cwd": "{PROJECT_PATH}"
    },
    "clean-queenbee": {
      "cmd": "scons -c",
      "sh": true,
      "cwd": "{PROJECT_PATH}"
    }
  }
}
